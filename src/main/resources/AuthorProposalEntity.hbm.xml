<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="domain.AuthorProposalEntity" table="AuthorProposal" schema="dbo" catalog="conference">
        <composite-id mapped="true" class="domain.AuthorProposalEntityPK">
            <key-property name="proposalId" column="ProposalID"/>
            <key-property name="authorId" column="AuthorID"/>
        </composite-id>
    </class>
</hibernate-mapping>