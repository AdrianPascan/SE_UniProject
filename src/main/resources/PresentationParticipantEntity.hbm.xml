<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="domain.PresentationParticipantEntity" table="PresentationParticipant" schema="dbo"
           catalog="conference">
        <composite-id mapped="true" class="domain.PresentationParticipantEntityPK">
            <key-property name="participantId">
                <column name="ParticipantID" sql-type="int"/>
            </key-property>
            <key-property name="presentationId">
                <column name="PresentationID" sql-type="int"/>
            </key-property>
            <key-property name="prRoleId">
                <column name="PrRoleID" sql-type="int"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>