<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="domain.ProposalEntity" table="Proposal" schema="dbo" catalog="conference">
        <id name="proposalId">
            <column name="ProposalID" sql-type="int"/>
            <generator class="identity"></generator>
        </id>
        <property name="keywords">
            <column name="Keywords" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="topic">
            <column name="Topic" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="abstractPaper">
            <column name="Abstract" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="fullPaper">
            <column name="FullPaper" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>