<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="domain.ProposalReviewEntity" table="ProposalReview" schema="dbo" catalog="conference">
        <composite-id mapped="true" class="domain.ProposalReviewEntityPK">
            <key-property name="pcMemberId">
                <column name="PCMemberID" sql-type="int"/>
            </key-property>
            <key-property name="proposalId">
                <column name="ProposalID" sql-type="int"/>
            </key-property>
        </composite-id>
        <property name="status">
            <column name="Status" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>