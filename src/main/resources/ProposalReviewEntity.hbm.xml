<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="domain.ProposalReviewEntity" table="ProposalReview" schema="dbo" catalog="conference">
        <composite-id mapped="true" class="domain.ProposalReviewEntityPK">
            <key-property name="pcMemberId" column="PCMemberID"/>
            <key-property name="proposalId" column="ProposalID"/>
        </composite-id>
        <property name="status" column="Status"/>
    </class>
</hibernate-mapping>