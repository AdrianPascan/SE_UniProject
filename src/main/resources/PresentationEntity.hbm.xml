<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="domain.PresentationEntity" table="Presentation" schema="dbo" catalog="conference">
        <id name="presentationId">
            <column name="PresentationID" sql-type="int"/>
            <generator class="identity"></generator>
        </id>
        <property name="section">
            <column name="Section" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="time">
            <column name="Time" sql-type="time" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>